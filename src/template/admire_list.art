<ul class='list_imgs'>
{{each list}}
  <li>
    <a>
      <div class="imgs">
        <img src="{{$value.imgrul}}" alt="">
      </div>
    </a>
  </li>
{{/each}}
</ul>
<div class='fenye'>
  {{each pageList}}
    {{if $index === 0}}
    <a  class="thisclass" name="{{$index}}">{{$index + 1}}</a>
    {{else}}
    <a  name="{{$index}}">{{$index + 1}}</a>
    {{/if}}
  {{/each}}
</div>